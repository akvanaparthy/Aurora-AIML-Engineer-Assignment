{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/ask",
      "dest": "src/index.ts",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/health",
      "dest": "src/index.ts",
      "methods": ["GET"]
    },
    {
      "src": "/stats",
      "dest": "src/index.ts",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"]
}
